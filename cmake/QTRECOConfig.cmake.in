# QtReco.cmake.in
#
# cmake config file for QTRECO
#  QTRECO_INCLUDE_DIRS  - the QTRECO include directory
#  QTRECO_LIBRARIES     - The libraries needed to use QTRECO
#  QTRECO_DEFINITIONS   - the compile time definitions


#fixme: not true if INCLUDE_INSTALL_DIR is absolute path
SET( QTRECO_INCLUDE_DIRS "@FULL_INCLUDE_INSTALL_DIR@" )
SET( QTRECO_INCLUDE_DIRS "${QTRECO_INCLUDE_DIRS}")

SET( QTRECO_LIBRARIES "@FULL_LIB_INSTALL_DIR@/@FULL_LIBRARY_NAME@" )

SET( QTRECO_DEFINITIONS @QTRECO_EXPORT_DEFINITIONS@ )

SET( QTRECO_ROOT_DIR "@CMAKE_INSTALL_PREFIX@" )

SET( QTRECO_MAJOR_VERSION "@QTRECO_MAJOR_VERSION@" )
SET( QTRECO_MINOR_VERSION "@QTRECO_MINOR_VERSION@" )
SET( QTRECO_PATCH_LEVEL    "@QTRECO_PATCH_LEVEL@"    )


SET( QTRECO_FOUND TRUE )